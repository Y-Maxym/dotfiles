configuration {
    window-title: "Rofi Launcher";
    border: true;
    border-width: 2;
    padding: 10;
    location: 1;
}

appearance {
    foreground: #f8f8f2;
    background: #282a36;
    selected-background: #44475a;
    selected-foreground: #50fa7b;
    urgent-background: #ff5555;
    urgent-foreground: #f8f8f2;
    font: "JetBrains Mono 12";
}

behavior {
    select-hold: false;
    cycle: true;
}

